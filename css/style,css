footer.site-footer {
  padding-bottom: 1.75rem;
  font-size: .875rem;
  color: #757575;
  line-height: 16px;
  font-weight: 400; }
  @media only screen and (max-width: 1560px) {
    footer.site-footer {
      font-size: .683rem;
      line-height: 14px; } }
  @media only screen and (max-width: 1440px) {
    footer.site-footer {
      font-size: .585rem;
      line-height: 12px;
      padding-bottom: 1.25rem; } }
